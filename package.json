{
  "name": "bilbomd-backend",
  "description": "backend server for BilboMD webapp",
  "version": "1.21.1",
  "main": "server.js",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist",
    "lint": "eslint",
    "tsc": "tsc",
    "build": "npm-run-all clean copy-assets lint tsc",
    "dev": "concurrently \"npm run watch-ts\" \"nodemon dist/server.js\"",
    "watch-ts": "tsc -w",
    "start": "npm run build && node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "server": "nodemon .",
    "prepare": "husky"
  },
  "dependencies": {
    "@bl1231/bilbomd-mongodb-schema": "^1.9.1",
    "@bull-board/api": "^6.12.7",
    "@bull-board/express": "^6.12.7",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "bullmq": "^5.58.5",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "dotenv": "^17.2.2",
    "eslint-plugin-jest": "^29.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.3.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.18.1",
    "mongoose-to-swagger": "^1.5.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "nodemailer-express-handlebars": "^7.0.0",
    "openid-client": "6.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.35.0",
    "@jest/globals": "^30.1.2",
    "@shelf/jest-mongodb": "^5.2.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^7.0.1",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.6",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4"
  }
}
