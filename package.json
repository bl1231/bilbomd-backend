{
  "name": "bilbomd-backend",
  "description": "backend server for BilboMD webapp",
  "version": "1.20.2",
  "main": "server.js",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist",
    "lint": "eslint",
    "tsc": "tsc",
    "build": "npm-run-all clean copy-assets lint tsc",
    "dev": "concurrently \"npm run watch-ts\" \"nodemon dist/server.js\"",
    "watch-ts": "tsc -w",
    "start": "npm run build && node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "server": "nodemon .",
    "prepare": "husky"
  },
  "dependencies": {
    "@bl1231/bilbomd-mongodb-schema": "^1.7.1",
    "@bull-board/api": "^6.9.6",
    "@bull-board/express": "^6.9.6",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.9.0",
    "bullmq": "^5.53.1",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "dotenv": "^16.5.0",
    "eslint-plugin-jest": "^28.12.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.6.0",
    "mongoose": "^8.15.1",
    "mongoose-to-swagger": "^1.5.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "nodemailer-express-handlebars": "^7.0.0",
    "openid-client": "6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@jest/globals": "^29.7.0",
    "@shelf/jest-mongodb": "^5.2.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.29",
    "@types/nodemailer": "^6.4.17",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.0",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.0"
  }
}
