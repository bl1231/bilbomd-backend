{
  "name": "bilbomd-backend",
  "description": "backend server for BilboMD webapp",
  "version": "1.2.0",
  "main": "server.js",
  "private": true,
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist",
    "lint": "eslint \"./src/**/*\" --ext .ts",
    "tsc": "tsc",
    "build": "npm-run-all clean copy-assets lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "npm run build && node dist/server.js",
    "test": "NODE_ENV=test jest --testTimeout=5000 --runInBand --verbose --detectOpenHandles",
    "server": "nodemon .",
    "generate-swagger": "node ./openapi/swagger.js"
  },
  "dependencies": {
    "@bull-board/api": "^5.9.1",
    "@bull-board/express": "^5.9.1",
    "bullmq": "^4.12.5",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "debug": "~4.3.4",
    "dotenv": "^16.3.1",
    "express": "~4.18.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.1",
    "express-rate-limit": "^7.1.2",
    "express-winston": "^4.2.0",
    "formidable": "2.1.2",
    "fs-extra": "^11.1.1",
    "handlebars": "^4.7.8",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.6.3",
    "mongoose-sequence": "^5.3.1",
    "mongoose-to-swagger": "^1.5.1",
    "nodemailer": "^6.9.7",
    "nodemailer-express-handlebars": "^6.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui": "^5.9.0",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.23.2",
    "@jest/globals": "^29.7.0",
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.8",
    "@types/swagger-ui-express": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^29.7.0",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.52.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "tslint": "^6.1.3"
  }
}
