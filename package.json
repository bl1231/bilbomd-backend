{
  "name": "bilbomd-backend",
  "description": "backend server for BilboMD webapp",
  "version": "1.10.0",
  "main": "server.js",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist",
    "lint": "eslint",
    "tsc": "tsc",
    "build": "npm-run-all clean copy-assets lint tsc",
    "dev": "concurrently \"npm run watch-ts\" \"nodemon dist/server.js\"",
    "watch-ts": "tsc -w",
    "start": "npm run build && node dist/server.js",
    "test": "NODE_ENV=test jest --testTimeout=5000 --runInBand --verbose --detectOpenHandles --coverage",
    "test:jobs": "NODE_ENV=test jest --testTimeout=5000 --runInBand --detectOpenHandles test/jobs.test.ts",
    "server": "nodemon .",
    "swagger": "npx ts-node src/openapi/swagger.ts"
  },
  "dependencies": {
    "@bl1231/bilbomd-mongodb-schema": "^1.0.23",
    "@bull-board/api": "^5.21.4",
    "@bull-board/express": "^5.21.4",
    "bullmq": "^5.12.13",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "date-fns": "^3.6.0",
    "debug": "~4.3.6",
    "dotenv": "^16.4.5",
    "eslint-plugin-jest": "^28.8.2",
    "express": "~4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.4.0",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "http-errors": "~2.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.6.1",
    "mongoose-to-swagger": "^1.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "nodemailer-express-handlebars": "^6.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui": "^5.17.14",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@jest/globals": "^29.7.0",
    "@shelf/jest-mongodb": "^4.3.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.3",
    "@types/nodemailer": "^6.4.15",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.1",
    "chai-http": "^5.0.0",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.30.0",
    "jest": "^29.7.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.4.0"
  }
}
