{
  "name": "bilbomd-backend",
  "description": "backend server for BilboMD webapp",
  "version": "1.8.0",
  "main": "server.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist",
    "lint": "eslint \"./src/**/*\" --ext .ts",
    "tsc": "tsc",
    "build": "npm-run-all clean copy-assets lint tsc",
    "dev": "concurrently \"npm run watch-ts\" \"nodemon dist/server.js\"",
    "watch-ts": "tsc -w",
    "start": "npm run build && node dist/server.js",
    "test": "NODE_ENV=test jest --testTimeout=5000 --runInBand --verbose --detectOpenHandles --coverage",
    "test:jobs": "NODE_ENV=test jest --testTimeout=5000 --runInBand --detectOpenHandles test/jobs.test.ts",
    "server": "nodemon .",
    "generate-swagger": "node ./openapi/swagger.js"
  },
  "dependencies": {
    "@bull-board/api": "^5.15.1",
    "@bull-board/express": "^5.15.1",
    "bullmq": "^5.4.5",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "date-fns": "^3.6.0",
    "debug": "~4.3.4",
    "dotenv": "^16.4.5",
    "express": "~4.19.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.2.0",
    "express-winston": "^4.2.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "http-errors": "~2.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.2.2",
    "mongoose-to-swagger": "^1.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemailer-express-handlebars": "^6.1.2",
    "npm-check-updates": "^16.14.17",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui": "^5.12.0",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.4.3",
    "uuid": "^9.0.1",
    "winston": "^3.12.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.3",
    "@jest/globals": "^29.7.0",
    "@shelf/jest-mongodb": "^4.2.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.14",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-jest": "^29.7.0",
    "chai": "^5.1.0",
    "chai-http": "^4.4.0",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.7.0",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2"
  }
}
